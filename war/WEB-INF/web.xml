<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.gwt.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/organaizer/greet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>tablepanelServlet</servlet-name>
    <servlet-class>com.gwt.server.tablepanel.TablePanelServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>tablepanelServlet</servlet-name>
    <url-pattern>/organaizer/tablepanel</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Organaizer.html</welcome-file>
  </welcome-file-list>

</web-app>
